<%--  Created by IntelliJ IDEA.  User: jessemcgilallen  Date: 2/22/16  Time: 2:38 PM  To change this template use File | Settings | File Templates.  This is a placeholder for the actual language page--%><%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%><c:import url="/jsp/doctype.jsp" /><html><c:set var="pageTitle" value="${language.name} | Programming Review" scope="request" /><c:import url="../jsp/header-tag.jsp" /><body><c:import url="../jsp/navigation-bar.jsp" /><h1>${language.name}</h1><ul class="nav nav-tabs">    <c:url value="/languages/update" var="updateUrl">        <c:param name="name" value="${language.name}" />    </c:url>    <li><a href="${updateUrl}">Update</a></li>    <c:url value="/languages/delete" var="deleteUrl">        <c:param name="name" value="${language.name}" />    </c:url>    <li><a href="${deleteUrl}">Delete</a></li>    <c:url value="/concepts/new" var="conceptUrl">        <c:param name="languageName" value="${language.name}" />    </c:url>    <li><a href="${conceptUrl}">Add Concept</a></li>    <c:url value="/terms/new" var="termUrl">        <c:param name="languageName" value="${language.name}" />    </c:url>    <li><a href="${termUrl}">Add Term</a></li></ul><h3><a href="#">Concepts</a></h3><c:forEach items="${concepts}" var="concept">    <div>        <c:set value="${concept.name}" var="conceptName" scope="page" />        <c:set value="${concept.description}" var="conceptDescription" scope="page" />        <c:url value="/concepts/show" var="url">            <c:param name="id" value="${concept.id}" />            <c:param name="language" value="${language}" />        </c:url>        <h3><a href="${url}">${conceptName}</a></h3>        <p>${conceptDescription}</p>        <hr />    </div></c:forEach><h3><a href="#">Terms</a></h3><c:forEach items="${terms}" var="term">    <div>        <c:set value="${term.name}" var="termName" scope="page" />        <c:set value="${term.description}" var="termDescription" scope="page" />        <c:url value="/terms/show" var="url">            <c:param name="id" value="${term.id}" />            <c:param name="language" value="${language.name}" />        </c:url>        <h3><a href="${url}">${termName}</a></h3>        <p>${termDescription}</p>        <hr />    </div></c:forEach></body></html>